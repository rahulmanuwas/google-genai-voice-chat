{
  "name": "@genai-voice/sdk",
  "version": "0.1.0",
  "description": "Unified SDK for genai-voice: chatbot React UI, LiveKit server/agent/react utilities, telephony adapters, and shared types",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./chatbot": {
      "types": "./dist/chatbot/index.d.ts",
      "import": "./dist/chatbot/index.mjs",
      "require": "./dist/chatbot/index.js"
    },
    "./chatbot/api": {
      "types": "./dist/chatbot/api.d.ts",
      "import": "./dist/chatbot/api.mjs",
      "require": "./dist/chatbot/api.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.mjs",
      "require": "./dist/agent/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./telephony": {
      "types": "./dist/telephony/index.d.ts",
      "import": "./dist/telephony/index.mjs",
      "require": "./dist/telephony/index.js"
    },
    "./telephony/telnyx": {
      "types": "./dist/telephony/adapters/telnyx.d.ts",
      "import": "./dist/telephony/adapters/telnyx.mjs",
      "require": "./dist/telephony/adapters/telnyx.js"
    },
    "./telephony/twilio": {
      "types": "./dist/telephony/adapters/twilio.d.ts",
      "import": "./dist/telephony/adapters/twilio.mjs",
      "require": "./dist/telephony/adapters/twilio.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "agent:dev": "node --env-file=../../apps/web/.env.local --import tsx src/agent/index.ts dev",
    "test": "node ./scripts/run-tests.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "livekit",
    "webrtc",
    "voice-agent",
    "gemini",
    "genai",
    "realtime",
    "voice-chat",
    "ai",
    "speech-to-speech",
    "react"
  ],
  "author": "Rahul Manuwas",
  "license": "MIT",
  "dependencies": {
    "livekit-server-sdk": "^2.15.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@google/genai": ">=1.0.0",
    "@livekit/agents": ">=1.0.0",
    "@livekit/agents-plugin-deepgram": ">=1.0.0",
    "@livekit/agents-plugin-google": ">=1.0.0",
    "@livekit/agents-plugin-silero": ">=1.0.0",
    "@livekit/components-react": ">=2.0.0",
    "@mariozechner/pi-coding-agent": ">=0.52.0",
    "livekit-client": ">=2.17.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@google/genai": {
      "optional": true
    },
    "@livekit/agents": {
      "optional": true
    },
    "@livekit/agents-plugin-deepgram": {
      "optional": true
    },
    "@livekit/agents-plugin-google": {
      "optional": true
    },
    "@livekit/agents-plugin-silero": {
      "optional": true
    },
    "@livekit/components-react": {
      "optional": true
    },
    "livekit-client": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@mariozechner/pi-coding-agent": {
      "optional": true
    }
  },
  "devDependencies": {
    "@google/genai": "^1.40.0",
    "@mariozechner/pi-agent-core": "^0.52.12",
    "@mariozechner/pi-ai": "^0.52.12",
    "@mariozechner/pi-coding-agent": "^0.52.12",
    "@livekit/agents-plugin-silero": "^1.0.44",
    "@types/node": "^22.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  }
}
