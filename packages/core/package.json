{
  "name": "@genai-voice/core",
  "version": "0.1.0",
  "description": "Shared types and conversation protocol for the genai-voice platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "genai-voice", "types", "conversation", "tools", "handoff",
    "guardrails", "knowledge", "analytics", "livekit", "ai"
  ],
  "author": "Rahul Manuwas",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
