{
  "name": "@genai-voice/react",
  "version": "0.4.0",
  "description": "Real-time voice and text chat React components for Google Gemini Live API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.mjs",
      "require": "./dist/api.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "node ./scripts/run-tests.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "google", "gemini", "genai", "voice-chat", "live-api",
    "ai", "chatbot", "react", "realtime", "audio"
  ],
  "author": "Rahul Manuwas",
  "license": "MIT",
  "sideEffects": false,
  "peerDependencies": {
    "@google/genai": ">=1.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@google/genai": "^1.40.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0"
  }
}
