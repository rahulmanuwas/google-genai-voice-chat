{
  "name": "google-genai-voice-chat",
  "version": "0.2.2",
  "description": "Real-time voice and text chat component for Google Gemini Live API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.mjs",
      "require": "./dist/api.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "google",
    "gemini",
    "genai",
    "voice-chat",
    "live-api",
    "ai",
    "chatbot",
    "react",
    "realtime",
    "audio"
  ],
  "author": "Rahul Manuwas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rahulmanuwas/google-genai-voice-chat.git"
  },
  "peerDependencies": {
    "@google/genai": ">=0.10.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@google/genai": "^0.10.0",
    "@types/node": "^25.0.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
