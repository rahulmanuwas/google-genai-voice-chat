{
  "name": "genai-voice",
  "version": "0.0.0",
  "private": true,
  "description": "AI voice agent platform powered by Google Gemini",
  "author": "Rahul Manuwas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rahulmanuwas/google-genai-voice-chat.git"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev agent:dev --filter=!@genai-voice/backend",
    "dev:convex": "turbo run dev --filter=@genai-voice/backend",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test --filter=./packages/*",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "tsup": "^8.0.0",
    "turbo": "^2.5.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.54.0"
  }
}
